// get two number from user and modulation assisgnment operator to print it on console
#include<stdio.h>
int getTwoNumberFromUser () {
    int number;
    scanf ("%d",&number);
    return number;
}
int modulationAssignmentOperator (int a,int b) {
    return (a%=b) ;
}
void main () {
    printf ("enter a first number ");
    int a = getTwoNumberFromUser ();
    printf ("enter a second number ") ;
    int b = getTwoNumberFromUser () ;
    int modulation = modulationAssignmentOperator(a,b);
    printf ("%d",modulation);
}
