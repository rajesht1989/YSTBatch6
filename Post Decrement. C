// get number from user and identify  post decrement number to print it on console
#include<stdio.h>
int getNumberFromUser(){
    int number;
    scanf ("%d", &number);
    return number;
}
int identifyPostdecrement (int a){
    printf ("before line %d\n",a--);
    printf ("afterline %d",a);
    return 0;
}
    void main (){
        printf ("enter your number ");
        int a = getNumberFromUser();
        int postdecrement = identifyPostdecrement (a);
        }
