
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int theNumberIsLessThanHundred(int a){
    if(a<100){
    printf(" the number is less than hundred ");    
    }else{
    printf(" the number is not less than hundred ");    
    }
}
void main(){
    printf(" enter a number ");
    int number=getNumberFromUser();
    theNumberIsLessThanHundred(number);
}
