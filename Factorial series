// get number from user and find the factorial series 
#include <stdio.h>
int getNumberFromUser(){
    int number;
    scanf("%d",&number);
    return number;
}
int findFactorial(int a){
  int c=1;
  for (int i=1;i<=a;i++){
      printf("%d",i);
  c=c*i;
  }
  return c;
}
void main(){
    printf(" enter the number ");
    int a=getNumberFromUser();
    int fact=findFactorial(a);
    printf("factorial is %d ",fact);
}
