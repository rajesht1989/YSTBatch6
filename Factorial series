// factorial series to program get number from user
#include <stdio.h>
int getNumberFromUser (){
int number;
scanf("%d", &number );
return number ;
}
int findFactorial(int a){
    int f=1;

for(int i=2;i<=a;i++){
    printf("%d", i);
     f=f*i;
}
return f;
}
void main (){
    printf("enter a number ");
    int a=getNumberFromUser();
        int fact=findFactorial(a);
    printf ("factorial is %d", fact );
    
}
