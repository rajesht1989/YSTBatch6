//get a number number from user subtract print it console
#include<stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int SubTwoNumber(int a,int b)
{
    return a-b;
}
void main()
{
   printf("enter your first number");
   int a=getNumberFromUser();
   printf("enter your second number");
   int b=getNumberFromUser();
   int subtract=SubTwoNumber(a,b);
   printf("subtraction%d",subtract);
}

    
