// Online C compiler to run C program online
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyCubeSurfaceArea(int a)
{
    return 6*(a*a);
}
void main()
{
    printf(" enter a value ");
    int a=getNumberFromUser();
    int cubesurfacearea=identifyCubeSurfaceArea(a);
    printf(" cubesurfacearea is %d",cubesurfacearea);
}
