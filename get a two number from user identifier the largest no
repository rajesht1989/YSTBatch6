
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyTheLargestNumber(int a,int b){
    if(a>b){
    printf(" a is largest number ");    
    }else{
    printf(" b is small number ");    
    }}
void main(){
    printf(" enter number ");
    int a,b=getNumberFromUser();
    identifyTheLargestNumber(a,b);
}
    

