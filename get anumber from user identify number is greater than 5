// Online C compiler to run C program online
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int numberIsGreaterThanFive(int a){
    if(a>=5){
    printf(" number is greater than five ");
    }else{
    printf(" number is not greater than five ");    
    }}
    void main(){
