// Online C compiler to run C program online
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int identifyTheRectangularArea(int l,int b)
{
    return (l*b);
}
int identifyTheRectangularCircumference(int l,int b)
{
    return 2*(l*b);
}
void main(){
printf(" enter a length "); 
int l =getNumberFromUser();
printf(" enter a breath "); 
int b =getNumberFromUser();
int rectangular =identifyTheRectangularArea(l,b);
printf("Rectangular Area is %d\n",rectangular);
int circumference =identifyTheRectangularCircumference(l,b);
printf("Rectangular Circumference is %d",circumference);
}
