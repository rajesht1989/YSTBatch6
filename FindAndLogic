// write a programe to get number from user and find the value of and logic print it on console
#include <stdio.h>
int getNumberFromUser(){
    int i;
    scanf("%d",&i);
    return i;
}
int findAndLogic(int a,int b,int c,int d){
    return ((a < b) && (c < d) && (c < a) && (d < b));
}
void main() {
    
    printf("Enter A First value ");
    int number1 = getNumberFromUser();
    printf("Enter A Second value ");
    int number2 = getNumberFromUser();
    printf("Enter A Third value ");
    int number3 = getNumberFromUser();
    printf("Enter A Forth value ");
    int number4 = getNumberFromUser();
int andLogic = findAndLogic(number1,number2,number3,number4);
   printf("The Value Is %d",andLogic);
    return 0;
}
