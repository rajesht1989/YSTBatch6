// write a program to get number from user and multiple them and find even number print it on console 
#include <stdio.h>
int getNumberFromUser(){
    int i;
    scanf("%d",&i);
    return i;
}
int multipleThem(int a,int b){
    return (a * b);
}
int findEvenNumber(int c){
    if (c % 2 == 0){
        printf("Its even number ");
    }else{
           printf("Its odd number ");
    }
}
void main() {
    printf("Enter a first number ");
    int number1 = getNumberFromUser();
    printf("Enter a first number ");
    int number2 = getNumberFromUser();
    int multiple =  multipleThem(number1,number2);
    printf ("multiple value is %d\n",multiple);
    findEvenNumber(multiple);
  
}
