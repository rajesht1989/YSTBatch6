// Online C compiler to run C program online
#include <stdio.h>
int getNumberFromUser(){
int side;
scanf("%d",&side);
return side;
}
int identifyTheSquareArea(int a)
{
    return (a*a);
}
int identifyTheCircumference(int a)
{
    return 4*(a*a);
}
void main(){
printf(" enter a value "); 
int number =getNumberFromUser();
int area=identifyTheSquareArea(number);
int circumference=identifyTheCircumference(number);
printf("area of value %d\n",area);
printf("Circumference of value %d",circumference);
}
