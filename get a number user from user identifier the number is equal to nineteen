
#include <stdio.h>
int getNumberFromUser(){
int number;
scanf("%d",&number);
return number;
}
int theNumberIsEqualToNineteen(int a){
    if(a==19){
    printf( " the number is equal to nineteen ");    
    }else{
    printf(" the number is not equal to nineteen " );    
    }}  
void main(){
    printf(" enter a number ");
    int number=getNumberFromUser();
    theNumberIsEqualToNineteen(number);
}
