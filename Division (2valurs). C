// get two number from user and division them print it on console
#include<stdio.h>
int getTwoNumberFromUser () {
    int number;
    scanf ("%d",&number);
    return number;
}
int divisionTwoNumberFromUser (int a,int b) {
    return a/b;
}
void main () {
    printf ("enter a first number");
    int a = getTwoNumberFromUser ();
    printf ("enter a second number") ;
    int b = getTwoNumberFromUser () ;
    int division = divisionTwoNumberFromUser(a,b);
    printf ("Division is%d",division);
}
