// get number from user and identify the preincrement operator to print it on console
#include<stdio.h>
int getNumberFromUser () {
    int number;
   scanf ("%d",& number);
   return number;
}
int identifyTheIncrementNumber(int a) {
    return ++a;
}
void main() {
    printf ("enter your number");
    int a = getNumberFromUser();
    int increment =  identifyTheIncrementNumber(a);
    printf ("value is%d", increment);
}
