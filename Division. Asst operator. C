// get two number from user and division assisgnment operator to print it on console
#include<stdio.h>
int getTwoNumberFromUser () {
    int number;
    scanf ("%d",&number);
    return number;
}
int divisionAssignmentOperator (int a,int b) {
    return (a/=b) ;
}
void main () {
    printf ("enter a first number ");
    int a = getTwoNumberFromUser ();
    printf ("enter a second number ") ;
    int b = getTwoNumberFromUser () ;
    int division = divisionAssignmentOperator(a,b);
    printf ("%d",division);
}
