// get two float number user and find number in words them print on console
#include<stdio.h>
int getNumberFromUser(){
    int number;
    printf("Enter a number ");
    scanf("%d",& number);
    return number;
}
int printNumberInWords(int  a){
    if (a == 1){
        printf("One");
    }
    else if (a == 2){
                printf("Two");

    }
      else if (a == 3){
                printf("Three");

    }  else if (a == 4){
                printf("Four");

    }  else if (a == 5){
                printf("Five");
}
  
}
void main(){
  
    int  a=getNumberFromUser();
   printNumberInWords(a);
}
