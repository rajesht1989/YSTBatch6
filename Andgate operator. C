// get number from user and identify the andgate operator to print it on console
#include<stdio.h>
int getNumberFromUser () {
    int number;
    scanf ("%d",&number);
    return number;
}
int identifyAndgateOperator (int a,int b,int c,int d){
return (a<b) && (c>d);
}
int main () {
    printf ("enter a first number ");
    int a = getNumberFromUser();
    printf ("enter a second number ");
    int b = getNumberFromUser();
    printf ("enter a third number ");
    int c = getNumberFromUser();
    printf ("enter a fourth number ");
    int d = getNumberFromUser();
    int Andgate = identifyAndgateOperator (a,b,c,d);
    printf ("Andgate is%d", Andgate);
}
