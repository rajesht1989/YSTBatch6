// Online C compiler to run C program online
#include <stdio.h>
float getNumberFromUser(){
float number;
scanf("%f",&number);
return number;
}
float identifyCylinderVolume(float r,float h)
{
    return 3.14*(r*r)*h;
}
void main()
{
  printf (" enter a radius ");
  float b=getNumberFromUser();
  printf (" enter a height ");
  float h=getNumberFromUser();
  float cylindervolume=identifyCylinderVolume(b,h);
  printf (" cylindervolune is %f",cylindervolume);
}
