// get number from user and identify the post increment number to print it on console
#include<stdio.h>
int getNumberFromUser(){
    int number;
    scanf ("%d", &number);
    return number;
}
int identifyPostIncrement (int a){
    printf ("before line %d",a++);
    printf ("afterline %d",a);
    return 0;
}
    void main (){
        printf ("enter your number ");
        int a = getNumberFromUser();
        int postincrement = identifyPostIncrement (a);
        }
